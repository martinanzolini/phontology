@prefix : <http://www.semanticweb.org/martina/ontologies/2020/PhOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pho: <http://www.semanticweb.org/martina/ontologies/2020/PhOntology> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://purl.org/vocab/vann/vann:> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix bibframe: <http://id.loc.gov/ontologies/bibframe/> .
@prefix PhOntology: <http://www.semanticweb.org/martina/ontologies/2020/PhOntology/> .
@prefix phontology: <http://www.semanticweb.org/martina/ontologies/2020/PhOntology#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/martina/ontologies/2020/PhOntology> .

<http://www.semanticweb.org/martina/ontologies/2020/PhOntology> rdf:type owl:Ontology ;
                                                                 owl:imports <http://www.ontologydesignpatterns.org/cp/owl/partof.owl> ,
                                                                             <http://www.ontologydesignpatterns.org/cp/owl/place.owl> ;
                                                                 dc:creator "Martina Anzolini"@en ;
                                                                 dc:date "Luglio 2020"@en ;
                                                                 dc:description "Ontology that describes a partial world of photography"@en ;
                                                                 dc:title "Photo Ontology"@en ;
                                                                 vann:preferredNamespacePrefix "pho"@en ;
                                                                 vann:preferredNamespaceUri "http://www.semanticweb.org/martina/ontologies/2020/PhOntology"@en ;
                                                                 rdfs:label "PhOntology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/vann:preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/vann:preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty ,
                                                                                       owl:TransitiveProperty ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                              rdfs:range owl:Thing ;
                                                                              rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"@en ;
                                                                              rdfs:label "has member"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Contains_public_photo
phontology:Contains_public_photo rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                                 rdfs:domain phontology:Reportage ;
                                 rdfs:range phontology:Photo ;
                                 rdfs:label "contains public photo"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Is_Contained
phontology:Is_Contained rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf phontology:is_member_of ;
                        rdfs:domain phontology:Photo ;
                        rdfs:range phontology:Album ;
                        rdfs:label "is contained"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasBrand
phontology:hasBrand rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( phontology:Lens
                                                phontology:Photocamera
                                              )
                                ] ;
                    rdfs:range phontology:Brand ;
                    rdfs:comment "brand about subject"@en ,
                                 "brand relativo al soggetto"@it ;
                    rdfs:label "has brand"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasCollaboratedWith
phontology:hasCollaboratedWith rdf:type owl:ObjectProperty ,
                                        owl:SymmetricProperty ,
                                        owl:TransitiveProperty ;
                               rdfs:domain phontology:Person ;
                               rdfs:range phontology:Person ;
                               rdfs:label "has collaborated with"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasLens
phontology:hasLens rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain phontology:Photocamera ;
                   rdfs:range phontology:Lens ;
                   rdfs:label "has lens"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasMainCharacter
phontology:hasMainCharacter rdf:type owl:ObjectProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain phontology:Photo ;
                            rdfs:range phontology:Person ;
                            rdfs:label "has main character"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasOwnCamera
phontology:hasOwnCamera rdf:type owl:ObjectProperty ;
                        rdfs:domain phontology:Person ;
                        rdfs:range phontology:Photocamera ;
                        owl:propertyChainAxiom ( PhOntology:hasTookPhoto
                                                 phontology:isRealizedWith
                                               ) ;
                        rdfs:label "has own camera"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasPartOfCamera
phontology:hasPartOfCamera rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                           rdf:type owl:TransitiveProperty ;
                           rdfs:domain phontology:Photocamera ;
                           rdfs:range phontology:PhotocameraComponent ;
                           rdfs:label "has part of camera"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasPhoto
phontology:hasPhoto rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                    rdfs:domain phontology:Album ;
                    rdfs:range phontology:Photo ;
                    rdfs:label "has photo"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasPlace
phontology:hasPlace rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation> ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:domain phontology:Photo ;
                    rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place> ;
                    rdfs:comment "luogo in cui accade un avvenimento"@it ;
                    rdfs:label "has place"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasRealizationDetail
phontology:hasRealizationDetail rdf:type owl:ObjectProperty ;
                                owl:propertyChainAxiom ( phontology:isRealizedWith
                                                         phontology:hasLens
                                                       ) ;
                                rdfs:label "has realization details"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasShutter
phontology:hasShutter rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain phontology:Photocamera ;
                      rdfs:range phontology:Shutter ;
                      rdfs:label "has shutter"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#hasSupport
phontology:hasSupport rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain phontology:Photocamera ;
                      rdfs:range phontology:SupportType ;
                      rdfs:label "has support"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#has_taken_in_city
phontology:has_taken_in_city rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf phontology:hasPlace ;
                             rdfs:domain phontology:Photo ;
                             rdfs:range phontology:City ;
                             rdfs:comment "città in cui viene realizzata una foto"@it ;
                             rdfs:label "has taken in city"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#has_taken_in_country
phontology:has_taken_in_country rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf phontology:hasPlace ;
                                rdf:type owl:TransitiveProperty ;
                                rdfs:domain phontology:Photo ;
                                rdfs:range phontology:Country ;
                                rdfs:comment "stato in cui viene realizzata una foto"@it ;
                                rdfs:label "has taken in country"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#isPartOfCamera
phontology:isPartOfCamera rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> ;
                          rdfs:domain phontology:PhotocameraComponent ;
                          rdfs:range phontology:Photocamera ;
                          rdfs:label "is part of photocamera"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#isPublished
phontology:isPublished rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf phontology:is_member_of ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain phontology:Photo ;
                       rdfs:range phontology:Reportage ;
                       rdfs:label "is published"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#isRealizedWith
phontology:isRealizedWith rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain phontology:Photo ;
                          rdfs:range phontology:Photocamera ;
                          rdfs:label "is realized with"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#is_member_of
phontology:is_member_of rdf:type owl:ObjectProperty ;
                        rdfs:domain owl:Thing ;
                        rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                        rdfs:label "is member of"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#is_realized_by
phontology:is_realized_by rdf:type owl:ObjectProperty ;
                          owl:inverseOf PhOntology:hasTookPhoto ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain phontology:Photo ;
                          rdfs:range phontology:Person ;
                          rdfs:label "is realized by"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology/hasTookPhoto
PhOntology:hasTookPhoto rdf:type owl:ObjectProperty ,
                                 owl:AsymmetricProperty ;
                        rdfs:domain phontology:Person ;
                        rdfs:range phontology:Photo ;
                        rdfs:label "has took photo"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Mirror
phontology:Mirror rdf:type owl:DatatypeProperty ;
                  rdfs:domain phontology:Photocamera ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "Mirror inside Photocamera"@en ;
                  rdfs:label "Mirror"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#PhotoDate
phontology:PhotoDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain phontology:Photo ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Date of Photo, espress in Years"@en ;
                     rdfs:label "Photo Date"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#cameraType
phontology:cameraType rdf:type owl:DatatypeProperty ;
                      rdfs:domain phontology:Photocamera ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:oneOf [ rdf:type rdf:List ;
                                               rdf:first "Analogical" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "Digital" ;
                                                          rdf:rest rdf:nil
                                                        ]
                                             ]
                                 ] ;
                      rdfs:comment "Type of Photocamera"@en ;
                      rdfs:label "Camera Type"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#focalLenght
phontology:focalLenght rdf:type owl:DatatypeProperty ;
                       rdfs:domain phontology:Lens ;
                       rdfs:range xsd:short ;
                       rdfs:comment "Focal Lenght of Lens"@en ;
                       rdfs:label "Focal Lenght"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#photoColorType
phontology:photoColorType rdf:type owl:DatatypeProperty ;
                          rdfs:domain phontology:Photo ;
                          rdfs:range [ rdf:type rdfs:Datatype ;
                                       owl:oneOf [ rdf:type rdf:List ;
                                                   rdf:first "B/W" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "Color" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "Neon" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "Sepia" ;
                                                                                    rdf:rest rdf:nil
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                     ] ;
                          rdfs:comment "Color about photograph"@en ;
                          rdfs:label "Photo Color Type"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology/photoTitle
PhOntology:photoTitle rdf:type owl:DatatypeProperty ;
                      rdfs:domain phontology:Photo ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Title of photograph"@en ;
                      rdfs:label "Photo Title" .


#################################################################
#    Classes
#################################################################

###  http://schema.org/Brand
<http://schema.org/Brand> rdf:type owl:Class ;
                          owl:equivalentClass phontology:Brand ;
                          rdfs:comment "A brand is a name used by an organization or business person for labeling a product, product group, or similar."@en ;
                          rdfs:isDefinedBy "https://schema.org/Brand"@en ;
                          rdfs:label "Brand"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class ;
                                                                               rdfs:comment "A quantity of anything collected together into a mass"@en ,
                                                                                            "quantità di qualcosa di ogni tipo raccolta insieme"@it ;
                                                                               rdfs:isDefinedBy "http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl" ;
                                                                               rdfs:label "Collection"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Album
phontology:Album rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty phontology:hasPhoto ;
                                   owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass phontology:Photo
                                 ] ;
                 rdfs:comment "A collection of personal photographs or other significative images that are arranged for viewing"@en ,
                              "Raccoglie fotografie riprese in circostanze varie e in momenti particolarm. significativi della vita"@it ;
                 rdfs:label "Album"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Brand
phontology:Brand rdf:type owl:Class ;
                 rdfs:comment "a name of an organization or business"@en ,
                              "nome di una organizzazione o business"@it ;
                 rdfs:label "Brand"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#City
phontology:City rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place> ;
                rdfs:comment "A town of significant size and importance, center of population and culture"@en ;
                rdfs:label "City"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Country
phontology:Country rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place> ;
                   rdfs:comment "Land, the territory of a nation or state"@en ;
                   rdfs:label "Country"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Film
phontology:Film rdf:type owl:Class ;
                rdfs:subClassOf phontology:SupportType ;
                rdfs:comment "A photographic material consisting of a base of celluloid covered with a photographic emulsion; used to make negatives or transparencies"@en ,
                             "materiale fotografico, constituito di celluloide, usato per realizzare negativi"@it ;
                rdfs:label "Film"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#FisheyeLens
phontology:FisheyeLens rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty phontology:focalLenght ;
                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                  owl:onDatatype xsd:short ;
                                                                  owl:withRestrictions ( [ xsd:maxInclusive "15"^^xsd:short
                                                                                         ]
                                                                                       )
                                                                ]
                                           ] ;
                       rdfs:subClassOf phontology:WideLens ;
                       rdfs:comment "type of wide lens"@en ;
                       rdfs:label "Fisheye Lens"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Lens
phontology:Lens rdf:type owl:Class ;
                rdfs:subClassOf phontology:PhotocameraComponent ;
                rdfs:comment "A device that is part of camera which focuses the image in a camera"@en ,
                             "device che è parte della fotocamera, permette di mettere a fuoco l'immagine"@it ;
                rdfs:label "Lens"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#MemoryCard
phontology:MemoryCard rdf:type owl:Class ;
                      rdfs:subClassOf phontology:SupportType ;
                      rdfs:comment "A portable card that stores data in flash memory, often used in digital cameras"@en ,
                                   "un mezzo portabile e piccolo, dove salvare dati, spesso usato nella fotografia digitale"@it ;
                      rdfs:label "Memory Card"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Mirrorless
phontology:Mirrorless rdf:type owl:Class ;
                      rdfs:subClassOf phontology:Photocamera ;
                      rdfs:comment "Kind of camera, it's compact and smart, it hasn't any mirror inside"@en ,
                                   "tipologia di fotocamera compatta, non ha uno specchio interno"@it ;
                      rdfs:label "Mirrorless"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Person
phontology:Person rdf:type owl:Class ;
                  owl:equivalentClass <http://xmlns.com/foaf/0.1/Person> ;
                  rdfs:comment "A person"@en ,
                               "una persona"@it ;
                  rdfs:label "Person"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Photo
phontology:Photo rdf:type owl:Class ;
                 owl:equivalentClass <https://www.wikidata.org/wiki/Q125191> ;
                 rdfs:comment "A representation of a person or scene recorded by a camera"@en ,
                              "Rappresentazione di una persona o scena catturata da una fotocamera"@it ;
                 rdfs:label "Photo"@en ;
                 owl:equivalentClass "https://www.wikidata.org/wiki/Q125191"@en ;
                 skos:broader "http://purl.org/dc/dcmitype/Image" .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Photocamera
phontology:Photocamera rdf:type owl:Class ;
                       rdfs:comment "A hand-held photographic device with an aperture controlled by a shutter that opens to admit light: focused by a lens, the light forms an image on a light-sensitive film or plate loaded through the back or top."@en ,
                                    "mezzo per realizzare foto, con un'apertura controllata da un otturatore che aprendosi fa entrare luce, messa a fuoco da una lente e dando vita ad un'immagine, caricata su un dispositivo di supporto digitale o analogico, come una pellicola"@it ;
                       rdfs:label "Photocamera"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#PhotocameraComponent
phontology:PhotocameraComponent rdf:type owl:Class ;
                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                  owl:onProperty phontology:isPartOfCamera ;
                                                  owl:someValuesFrom phontology:Photocamera
                                                ] ;
                                rdfs:comment "Equipment used by a photographer for taking photographs"@en ,
                                             "componenti relativi alla fotocamera, usati dal fotografo per realizzare foto"@it ;
                                rdfs:label "Photocamera Component"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Photographer
phontology:Photographer rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty PhOntology:hasTookPhoto ;
                                              owl:someValuesFrom [ owl:intersectionOf ( phontology:Photo
                                                                                        [ rdf:type owl:Restriction ;
                                                                                          owl:onProperty phontology:isPublished ;
                                                                                          owl:someValuesFrom phontology:Reportage
                                                                                        ]
                                                                                      ) ;
                                                                   rdf:type owl:Class
                                                                 ]
                                            ] ;
                        rdfs:subClassOf phontology:Person ;
                        rdfs:comment "A person who takes photographs, either as a job or hobby"@en ,
                                     "una persona che realizza fotografie, come lavoro o hobby"@it ;
                        rdfs:label "Photographer"@en ;
                        owl:equivalentClass "https://www.wikidata.org/wiki/Q33231" .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#PortraitPhoto
phontology:PortraitPhoto rdf:type owl:Class ;
                         rdfs:subClassOf phontology:Photo ;
                         rdfs:label "Portrait Photo"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Reflex
phontology:Reflex rdf:type owl:Class ;
                  rdfs:subClassOf phontology:Photocamera ;
                  rdfs:comment "Kind of camera, it has a mirror inside that reflect image"@en ,
                               "tipologia di fotocamera avente uno specchio interno"@it ;
                  rdfs:label "Reflex"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Reportage
phontology:Reportage rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                     rdfs:comment "Il servizio di un fotoreporter, fotocronaca"@it ,
                                  "The reporting of news or information of general interest by fotoreporter"@en ;
                     rdfs:label "Reportage"@en .


###  http://www.semanticweb.org/martina/ontologies/2020/PhOntology#Shutter
phontology:Shutter rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( phontology:Electronical
                                                     phontology:Mechanical
                                                   )
                                       ] ;
                   rdfs:subClassOf phontology:PhotocameraComponent ;
